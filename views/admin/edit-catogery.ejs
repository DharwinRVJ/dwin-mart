<%- include('../layouts/adminheader.ejs') %>
<h4 class="page-title my-2">Edit Catogery</h4>
<a href="/admin/catogeries" class="btn btn-sm btn-primary mb-3">Back to All Catogeries</a>
<form method="POST" action="/admin/catogeries/edit-catogery" autocomplete="off">
    <div class="form-row">
        <div class="form-group col-md-4 col-sm-12">
            <label for="title">Title</label>
            <input required type="text" class="form-control" name="title" value="<%= title %>" id="title"
                placeholder="Title">
        </div>
        <div class="form-group col-md-4 col-sm-12">
            <label for="slug">Slug</label>
            <input required type="text" class="form-control" name="slug" value="<%= slug %>" id="slug"
                placeholder="Slug">
        </div>
    </div>
    <input type="hidden" name="id" value="<%= id %>">
    <button type="submit" class="btn btn-sm btn-outline-primary">Submit</button>
</form>

<%- include('../layouts/adminfooter.ejs') %>